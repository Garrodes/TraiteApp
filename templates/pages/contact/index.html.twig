{% extends 'base.html.twig' %}

{% block title %} {{parent()}}| Contact Us {% endblock %}

{% block body %}
<div class="container mt-4">
    {% for message in app.flashes('success') %}
    <div class="alert-success">
           {{ message }}
   </div>
   {% endfor %}
   {% for message in app.flashes('error') %}
   <div class="alert-success">
          {{ message }}
  </div>
  {% endfor %}
{{form(form)}}
</div>
{% endblock %}
