{% extends "base.html.twig" %}

{% block body %}

<div class="container">
    <h1> Modification du Relev√© </h1>
    {{ form_start(form)}}
    <div class="form-group">    
            {{ form_label(form.volume)}}
            {{ form_widget(form.volume)}}
        <div class="form-error">
            {{form_errors(form.volume)}}
        </div>          
      </div>
      <div class="form-group">
        {{ form_label(form.date)}}
        {{ form_widget(form.date)}}
